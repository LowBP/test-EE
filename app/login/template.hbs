<form>
  <label for="username">
    Login
  </label>
  {{input id="username" placeholder="Enter Login" value=username}}
  <label for="password">
    Password
  </label>
  {{input
    id="password"
    placeholder="Enter Password"
    type="password"
    value=password
  }}
  {{!-- <button {{action "authenticate"}}>
    Login
  </button> --}}
  <Button @intent="primary" @onClick={{action "authenticate"}}>
    Login
  </Button>
</form>